{
  "log": {
    "loglevel": "warning"
  },
  "inbounds": [
    {
      "tag": "local",
      "port": 4000,
      "protocol": "dokodemo-door",
      "settings": {
        "address": "127.0.0.1",
        "port": 4000,
        "network": "tcp,udp"
      }
    }
  ],
  "outbounds": [
    {
      "tag": "remote",
      "protocol": "vmess",
      "settings": {
        "vnext": [
          {
            "address": "proxy address",
            "port": 443,
            "users": [
              {
                "id": "uuid"
              }
            ]
          }
        ]
      },
      "streamSettings": {
        "network": "ws",
        "security": "tls",
        "wsSettings": {
          "path": "/v2rayclient"
        }
      }
    }
  ],
  "routing": {
    "rules": [
      {
        "type": "field",
        "inboundTag": [
          "local"
        ],
        "outboundTag": "remote"
      }
    ]
  }
}