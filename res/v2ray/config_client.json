{
  "log": {
    "loglevel": "warning"
  },
  "inbounds": [
    {
      "tag": "vin",
      "protocol": "http",
      "listen": "127.0.0.1",
      "port": 8080,
      "settings": {}
    }
  ],
  "outbounds": [
    {
      "tag": "socks-out",
      "protocol": "socks",
      "settings": {
        "servers": [
          {
            "address": "www.baidu.com",
            "port": 443,
            "users": [
              {
                "user": "my-username",
                "pass": "my-password"
              }
            ]
          }
        ]
      },
      "streamSettings": {
        "network": "ws",
        "security": "tls",
        "wsSettings": {
          "path": "/v2ray"
        }
      }
    }
  ]
}