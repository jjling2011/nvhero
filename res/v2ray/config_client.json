{
  "log": {
    "loglevel": "warning"
  },
  "inbound": {
    "port": 1080,
    "listen": "127.0.0.1",
    "protocol": "socks",
    "settings": {
      "auth": "noauth",
      "udp": false,
      "ip": "127.0.0.1"
    }
  },
  "outbound": {
    "tag": "agentout",
    "protocol": "vmess",
    "settings": {
      "vnext": [
        {
          "address": "your_app_name.herokuapp.com",
          "port": 443,
          "users": [
            {
              "id": "your_userid(xxxx-xxxx-...)",
              "alterId": 32,
              "security": "auto",
              "level": 0
            }
          ]
        }
      ]
    },
    "streamSettings": {
      "network": "ws",
      "security": "tls",
      "tlsSettings": {
        "allowInsecure": false
      },
      "wsSettings": {
        "connectionReuse": true,
        "path": "/your_web_socket_path"
      }
    },
    "mux": {
      "enabled": true,
      "concurrency": 8
    }
  }
}