{
  "name": "nvhero",
  "description": "Deploy nginx and v2ray on Heroku.",
  "keywords": [
    "nvhero"
  ],
  "env": {
    "IN_UUID": {
      "description": "inbound vmess uuid",
      "value": "f6349dcc-347f-4b95-843b-501df0ae176b"
    },
    "IN_WSPATH": {
      "description": "inbound ws path",
      "value": "/v2ray"
    },
    "REMOTE_SERVER": {
      "description": "on/off",
      "value": "off"
    },
    "OUT_SERVER": {
      "description": "remote server address",
      "value": "www.baidu.com"
    },
    "OUT_UUID": {
      "description": "remote vmess UUID",
      "value": "f6349dcc-347f-4b95-843b-501df0ae176b"
    },
    "OUT_WSPATH": {
      "description": "remote server ws path",
      "value": "/v2ray"
    }
  },
  "website": "https://github.com/jjling2011/nvhero",
  "repository": "https://github.com/jjling2011/nvhero",
  "stack": "container"
}
