{
  "name": "nvhero",
  "description": "Deploy nginx and v2ray on Heroku.",
  "keywords": [
    "nvhero"
  ],
  "env": {
    "VER": {
      "description": "输入v2ray-core版本",
      "value": "4.8.0"
    },
    "WSPATH": {
      "description": "ws路径,作死就用默认值",
      "value": "/v2ray"
    },
    "UUID": {
      "description": "Vmess的UUID即密码,作死就用默认值",
      "value": "8fd6dadc-0d59-4ae7-93bb-e6b8a231c2c2"
    }
  },
  "website": "https://github.com/jjling2011/nvhero",
  "repository": "https://github.com/jjling2011/nvhero",
  "stack": "container"
}
