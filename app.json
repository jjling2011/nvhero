{
  "name": "nvhero",
  "description": "Deploy nginx and v2ray on Heroku.",
  "keywords": [
    "nvhero"
  ],
  "env": {
    "IN_USERNAME": {
      "description": "inbound socks username",
      "value": "username"
    },
    "IN_PASSWORD": {
      "description": "inbound socks password",
      "value": "password"
    },
    "IN_WSPATH": {
      "description": "inbound ws path",
      "value": "/v2ray"
    },
    "REMOTE_SERVER": {
      "description": "on/off",
      "value": "off"
    },
    "OUT_SERVER": {
      "description": "remote server address",
      "value": "www.baidu.com"
    },
    "OUT_USERNAME": {
      "description": "remote server username",
      "value": "username"
    },
    "OUT_PASSWORD": {
      "description": "remote server password",
      "value": "password"
    },
    "OUT_WSPATH": {
      "description": "remote server ws path",
      "value": "/v2ray"
    }
  },
  "website": "https://github.com/jjling2011/nvhero",
  "repository": "https://github.com/jjling2011/nvhero",
  "stack": "container"
}
